<Project Sdk="Uno.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0-windows10.0.19041;net8.0</TargetFrameworks>
    <OutputType>Exe</OutputType>
    <UnoSingleProject>true</UnoSingleProject>

    <!-- Display name -->
    <ApplicationTitle>Litteratus</ApplicationTitle>
    <!-- App Identifier -->
    <ApplicationId>com.companyname.litteratus</ApplicationId>
    <!-- Versions -->
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
    <ApplicationVersion>1</ApplicationVersion>
    <!-- Package Publisher -->
    <ApplicationPublisher>Litteratus</ApplicationPublisher>
    <!-- Package Description -->
    <Description>Litteratus powered by Uno Platform.</Description>
  </PropertyGroup>
</Project>